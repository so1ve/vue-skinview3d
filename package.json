{
	"name": "vue-skinview3d",
	"version": "0.3.0",
	"packageManager": "pnpm@8.12.1",
	"author": "Ray <i@mk1.io> (@so1ve)",
	"description": "Three.js powered Minecraft skin viewer. (Vue binding of skinview3d)",
	"keywords": [
		"canvas",
		"minecraft",
		"skin"
	],
	"homepage": "https://github.com/so1ve/vue-skinview3d#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/so1ve/vue-skinview3d.git"
	},
	"bugs": {
		"url": "https://github.com/so1ve/vue-skinview3d/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./animations": {
			"require": "./dist/animations.cjs",
			"import": "./dist/animations.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "unbuild",
		"build:play": "npm -C playground run build",
		"dev": "nr play",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"play": "npm -C playground run dev",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"typecheck": "vue-tsc --noEmit"
	},
	"dependencies": {
		"skinview3d": "3.0.1"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.12",
		"@so1ve/eslint-config": "^1.0.1",
		"@so1ve/prettier-config": "^1.0.1",
		"@types/node": "^20.10.5",
		"@vue/runtime-core": "^3.3.12",
		"bumpp": "^9.2.1",
		"eslint": "^8.56.0",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3",
		"unbuild": "^2.0.0",
		"vue-tsc": "^1.8.25"
	},
	"peerDependencies": {
		"vue": "^3.3.4"
	}
}
